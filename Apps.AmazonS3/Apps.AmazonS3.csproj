<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Product>Amazon S3</Product>
    <Description>Object storage built to retrieve any amount of data from anywhere</Description>
    <Version>1.1.7</Version>
    <AssemblyName>Apps.AmazonS3</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Blackbird.Applications.Sdk.Common" Version="2.3.2-alpha1" />
    <PackageReference Include="Blackbird.Applications.SDK.Extensions.FileManagement" Version="1.0.1" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.301.1" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.300.6" />
    <PackageReference Include="Blackbird.Applications.Sdk.Utils" Version="1.0.13" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource CopyToOutputDirectory="Always" Include="image\icon.png" />
  </ItemGroup>
</Project>
