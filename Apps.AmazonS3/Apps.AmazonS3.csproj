<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Product>Amazon S3</Product>
    <Description>Object storage built to retrieve any amount of data from anywhere</Description>
    <Version>1.0.2</Version>
    <AssemblyName>Apps.AmazonS3</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.7.300.4" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.300.4" />
    <PackageReference Include="Blackbird.Applications.Sdk.Utils" Version="1.0.13" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource CopyToOutputDirectory="Always" Include="image\icon.png" />
  </ItemGroup>
</Project>