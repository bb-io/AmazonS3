<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Product>Amazon S3</Product>
    <Description>Object storage built to retrieve any amount of data from anywhere</Description>
    <Version>2.0.0</Version>
    <AssemblyName>Apps.AmazonS3</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Blackbird.Applications.SDK.Blueprints" Version="1.0.0" />
    <PackageReference Include="Blackbird.Applications.SDK.Extensions.FileManagement" Version="1.0.2-alpha3" />
	  <PackageReference Include="Blackbird.Applications.Sdk.Utils" Version="1.0.26" />
	  <PackageReference Include="AWSSDK.S3" Version="3.7.421.2" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.400.177" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource CopyToOutputDirectory="Always" Include="image\icon.png" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="..\README.md">
      <Link>README.md</Link>
    </Content>
  </ItemGroup>
</Project>
